    "redux-thunk": "^2.1.0",
    "require-hacker": "^2.1.3",
    "rimraf": "^2.4.3",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "stylelint": "^6.8.0",
    "stylelint-webpack-plugin": "^0.2.0",
    "ts-babel-node": "^1.0.0",
    "ts-helpers": "^1.1.1",
    "ts-node": "^0.9.0",
    "tslint": "^3.7.4",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.0",
    "webpack-split-by-path": "0.0.10",
    "whatwg-fetch": "^1.0.0"
  }
}
